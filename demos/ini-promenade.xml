<?xml version="1.0" encoding="UTF-8"?>

<JPSgcfm project="JPS-Project" version="0.4">
	<!-- seed used for initialising random generator -->
	<seed>12542</seed>
	<!-- geometry file -->
	<!--
	<geometry>inputfiles/arena/Raum080a.jul.xml</geometry>
	-->
	<geometry>inputfiles/arena/promenade.jul.xml</geometry>
	<!--persons information and distribution -->
	<person>inputfiles/arena/persons.xml</person>
	<!-- extra routing information -->
	<routing>inputfiles/arena/routing.xml</routing>
	<!-- traffic information: e.g closed doors -->
	<traffic>inputfiles/arena/traffic.xml</traffic>
	<!-- trajectories file and format -->
	<trajectories format="xml-plain" version="0.4">outputfiles/trajectories.xml
	</trajectories>
	<!-- where to store the logs 
	<logfile>outputfiles/log.txt</logfile>
 	-->

	<!-- These parameters may be overwritten -->
	<parameters>
		<tmax unit="sec">400</tmax>
		<solver>euler</solver>
		<stepsize>0.001</stepsize>
		<linkedcells enabled="true" cell_size="2.2"/>
		<routeChoiceStrategy>dummy</routeChoiceStrategy>
		<v0 mu="1.24" sigma="0.001" />
		<bmax mu="0.25" sigma="0.001" />
		<bmin mu="0.20" sigma="0.001" />
		<amin mu="0.18" sigma="0.001" />
		<tau mu="0.5" sigma="0.001" />
		<atau mu="0.5" sigma="0.001" />
		<force_ped nu="0.3" dist_max="3" disteff_max="2"
			interpolation_width="0.1" />
		<force_wall nu="0.2" dist_max="3" disteff_max="2"
			interpolation_width="0.1" />
	</parameters>
</JPSgcfm>
